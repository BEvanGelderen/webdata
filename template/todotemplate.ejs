<!DOCTYPE html>
<html>
<head>
	<link href="css.css" rel="stylesheet" type="text/css">
	<link rel="shortcut icon" type="image/x-icon" href="img/favicon2.ico">
	<link >
	<title>do.it</title>
</head>
<body>
	<nav><div class="nav"><img src="img/bell.png" id="sortDate"><img src="img/star2.png" id="backToNormal"></div></nav>	
	<main>
		<div class="container">
			<div class="content">
				<div class="highPriority"><h1>high</h1>
					<ul id="high">
						<% todo_array.forEach(function(todo) { if(todo.priority){console.log(todo)%>
						<li class= "todo" id= <%=todo.Id%>  datetime= <%=todo.duedate %>><%=todo.todo%><input type="checkbox" class="done"><img src="img/star2_bright.png" class="star"></li><button class="delete"> Delete</button><button class="edit">Edit</button><time><%=todo.duedate %></time>
						<% }}) %>
						<p class="h-plus"><img src="img/plus.png" ></p>
						<input type="text" id="highplus" class="h-input"><br/> 
						<input type="date" id="highdate" class="h-input"><br/>
						<button id="highbutton" class="h-input">+</button>
					</ul>
				</div>
				<div class="lowPriority"><h1>normal</h1>
					<ul id="low">
						<% todo_array.forEach(function(todo) { if(!todo.priority){ console.log(todo)%>
						<li class= "todo" id= <%=todo.Id%>  datetime= <%=todo.duedate %>><%=todo.todo%><input type="checkbox" class="done"><img src="img/star_bright_empty.png" class="emptyStar"></li><button class="delete"> Delete</button><button class="edit">Edit</button><time><%=todo.duedate %></time> 
						<% }}) %>
						<p class="l-plus"><img src="img/plus.png"></p>
						<input type="text" id="lowplus" class="l-input"><br/>
						<input type="date" id="lowdate" class="l-input"><br/>
						<button id="lowbutton" class="l-input">+</button>
					</ul>
					<a href="analytics">analytics</a>
				</div>
			</div>
		</div>
		<footer></footer>
	</main>
<script src='https://code.jquery.com/jquery-3.1.0.min.js'></script>
<script src='features.js'></script>
</body>
</html>
